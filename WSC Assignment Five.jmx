                <boolProp name="resetInterpreter">false</boolProp>
                <stringProp name="script">try{
	String availableBalanceString = vars.get(&quot;availableBalance&quot;);
	String remainingAmountString = vars.get(&quot;remainingAmount&quot;);
	double availableBalanceValue = Double.parseDouble(availableBalanceString);
	
	for(int i=0;i&lt;remainingAmountString.length();i++) {
		if(remainingAmountString.charAt(i) ==&apos;,&apos;) {
			remainingAmountString = remainingAmountString.substring(0,i)+remainingAmountString.substring(i+1,remainingAmountString.length());
		}
	}
	double remainingAmountValue = Double.parseDouble(remainingAmountString);

	double amountToPay;
	if(availableBalanceValue &gt; remainingAmountValue) {
		amountToPay = remainingAmountValue;
	} else {
		amountToPay = availableBalanceValue;
	}
	vars.put(&quot;amountToPay&quot;,String.valueOf(amountToPay));
}
catch (Throwable e){
    log.error(&quot;Errror in Beanshell&quot;, e);
	    throw e;
}</stringProp>
